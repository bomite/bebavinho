    <title>bebavinho @ViewBag.Title</title>
    <link href="~/Content/screen.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/screen_menu.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/font.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/jquery-ui.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/jquery.ui.dialog.min.css" rel="stylesheet" type="text/css" />
