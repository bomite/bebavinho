                        <a href="@Url.Content("~/Default/Sobre")">Sobre</a>
                    </li>
                    <li>
                        <a href="@Url.Content("~/Default/Contato")">Contato</a>
                    </li>
                </ul>
            </div>           
        </div>
    </nav>


    <div class="container">
        <div class="app">
            @RenderBody()
            <hr />
            <footer class="row">
                <p class="Static_18">&copy; @DateTime.Now.Year - Copyright bebavinho.com.br - Francesco Cekin - Victor Moraes</p>
            </footer>
